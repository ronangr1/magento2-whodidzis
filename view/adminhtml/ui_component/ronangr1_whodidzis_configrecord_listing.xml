<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">ronangr1_whodidzis_configrecord_listing.ronangr1_whodidzis_configrecord_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>ronangr1_whodidzis_configrecord_columns</spinner>
		<deps>
			<dep>ronangr1_whodidzis_configrecord_listing.ronangr1_whodidzis_configrecord_listing_data_source</dep>
		</deps>
	</settings>
	<dataSource name="ronangr1_whodidzis_configrecord_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">entity_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Ronangr1_WhoDidZis::ConfigRecord</aclResource>
		<dataProvider name="ronangr1_whodidzis_configrecord_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>entity_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="ronangr1_whodidzis_configrecord_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">ronangr1_whodidzis_configrecord_listing.ronangr1_whodidzis_configrecord_listing.ronangr1_whodidzis_configrecord_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">entity_id</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">ronangr1_whodidzis_configrecord_listing.ronangr1_whodidzis_configrecord_listing.ronangr1_whodidzis_configrecord_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>entity_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="entity_id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">-
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">10</item>
                </item>
            </argument>
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="author">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">-
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">20</item>
                </item>
            </argument>
			<settings>
				<filter>text</filter>
				<label translate="true">Author</label>
			</settings>
		</column>
        <column name="scope">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">-
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">50</item>
                </item>
            </argument>
            <settings>
                <filter>text</filter>
                <label translate="true">Scope</label>
            </settings>
        </column>
		<column name="path">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">-
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">30</item>
                </item>
            </argument>
			<settings>
				<filter>text</filter>
				<label translate="true">Path</label>
			</settings>
		</column>
		<column name="old_value">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">-
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">40</item>
                    <item name="resizeEnabled" xsi:type="boolean">false</item>
                    <item name="resizeDefaultWidth" xsi:type="number">200</item>
                </item>
            </argument>
			<settings>
				<filter>text</filter>
				<label translate="true">Old Value</label>
			</settings>
		</column>
        <column name="new_value">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">-
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">40</item>
                    <item name="resizeEnabled" xsi:type="boolean">false</item>
                    <item name="resizeDefaultWidth" xsi:type="number">200</item>
                </item>
            </argument>
            <settings>
                <filter>text</filter>
                <label translate="true">New Value</label>
            </settings>
        </column>
		<column name="recorded_at">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">-
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="sortOrder" xsi:type="number">60</item>
                </item>
            </argument>
			<settings>
				<filter>text</filter>
				<label translate="true">Recorded At</label>
			</settings>
		</column>
        <actionsColumn name="record_actions" class="Ronangr1\WhoDidZis\Ui\Component\Listing\Column\ConfigRecordActions">
            <settings>
                <indexField>entity_id</indexField>
                <resizeEnabled>false</resizeEnabled>
                <resizeDefaultWidth>107</resizeDefaultWidth>
                <fieldClass>
                    <class name="revert-action">true</class>
                </fieldClass>
            </settings>
        </actionsColumn>
	</columns>
</listing>
